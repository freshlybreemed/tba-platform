{
  "name": "one",
  "version": 2,
  "scope": "tba-project",
  "builds": [
    {
      "src": "server/handlers/get.js",
      "use": "@now/node"
    },
    {
      "src": "server/handlers/post.js",
      "use": "@now/node"
    },
    {
      "src": "package.json",
      "use": "@now/next"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "methods": ["POST"],
      "dest": "server/handlers/post.js"
    },
    {
      "src": "/api/connect",
      "methods": ["POST"],
      "dest": "server/helpers/user.js"
    },
    {
      "src": "/api/charge/(.*)",
      "methods": ["GET"],
      "dest": "server/handlers/get.js?id=$1"
    },
    {
      "src": "/api/event/(.*)",
      "methods": ["GET"],
      "dest": "server/handlers/get.js?slug=$1"
    },
    {
      "src": "/api/events/(.*)",
      "methods": ["GET"],
      "dest": "server/handlers/get.js?sub=$1"
    },
    {
      "src": "/api/user/(.*)",
      "methods": ["GET"],
      "dest": "server/handlers/get.js?sub=$1"
    },
    {
      "src": "/api/balance/(.*)",
      "methods": ["GET"],
      "dest": "server/handlers/get.js?id=$1"
    },
    { "src": "/static/(.*)", "dest": "/static/$1" },
    { "src": "/(.*)", "dest": "/$1" },
    { "src": "/event/:slug", "dest": "pages/event.js" }
  ],

  "env": {
    "GUEST_PASS_KEY": "564db0117b8a65ddd60271eaf3298aa4",
    "MONGO_URL": "@mongo-url",
    "STRIPE_SECRET_DEV": "@stripe-secret-dev",
    "STRIPE_CLIENT_DEV": "@stripe-client-dev",
    "STRIPE_SECRET_PROD": "@stripe-secret-prod",
    "AUTH0_CLIENT_ID": "@auth0-client-id"
  }
}
